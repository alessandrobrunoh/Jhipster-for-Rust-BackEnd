use axum::{
    extract::State,
    http::StatusCode,
};
use crate::di::app_state::AppState;
use std::sync::Arc;

#[utoipa::path(
    get,
    path = "/health",
    responses(
        (status = 200, description = "Service is healthy"),
    ),
    tag = "Health"
)]
pub async fn health_check(
    State(_app_state): State<Arc<AppState>>,
) -> StatusCode {
    StatusCode::OK
}
